{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Oswald",
							"fontStyle": "normal",
							"fontWeight": "200 300 400 500 600 700",
							"src": "file:./assets/fonts/Oswald/Oswald-VariableFont.woff2"
						}
					],
					"fontFamily": "Oswald, sans-serif",
					"name": "Oswald",
					"slug": "oswald"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Open Sans\"",
							"fontStyle": "normal",
							"fontWeight": "300 400 500 600 700 800",
							"src": "file:./assets/fonts/Open_Sans/OpenSans.woff2"
						},
						{
							"fontFamily": "\"Open Sans\"",
							"fontStyle": "italic",
							"fontWeight": "300 400 500 600 700 800",
							"src": "file:./assets/fonts/Open_Sans/OpenSans.woff2"
						}
					],
					"fontFamily": "\"Open Sans\", sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.8rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.4rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--open-sans)",
							"fontSize": "var(--wp--preset--font-size--medium)",
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"color": {
			"background": "#001a28",
			"gradient": "linear-gradient(90deg,rgb(0,26,40) 0%,rgb(6,39,57) 25%,rgb(6,39,57) 75%,rgb(0,26,40) 100%)",
			"text": "#dafcff"
		},
		"css": ".masthead-shadow {\n\tposition: relative;\n}\n.masthead-shadow::after {\n\tbottom: -5px;\n\tbox-shadow: inset 0 4px 8px -3px rgb(17, 17, 17);\n\tcontent: \"\";\n\theight: 5px;\n\tleft: 0px;\n\tpointer-events: none;\n\tposition: absolute;\n\tright: 0px;\n\twidth: 100%;\n\tz-index: 1000;\n}",
		"elements": {
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.55"
				}
			},
			"link": {
				"color": {
					"text": "#889da0"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"lineHeight": "1.55"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}
